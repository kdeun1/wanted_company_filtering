{
  "manifest_version": 3,
  "name": "아이원티드댓컴퍼니",
  "version": "1.1",
  "description": "원티드 채용공고 필터링 도구",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.wanted.co.kr/wdlist*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
